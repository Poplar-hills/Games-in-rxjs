{
  "name": "games-in-rxjs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -r dist/*",
    "test": "ava -w --verbose",
    "build:css": "stylus -w src/style.styl -o dist/style.css",
    "build:js": "watchify src/main.js --debug -t babelify -o dist/bundle.js -v",
    "predev": "echo 'OPEN index.html IN YOUR BROWSER ðŸ‘ˆ ðŸ‘ˆ'",
    "dev": "concurrently 'npm run build:css' 'npm run build:js'"
  },
  "author": "Poplar_hills",
  "license": "ISC",
  "dependencies": {
    "ramda": "^0.24.1",
    "rxjs": "^5.4.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "concurrently": "^3.4.0",
    "stylus": "^0.54.5",
    "watchify": "^3.9.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "files": [
      "test/*.spec.js"
    ],
    "source": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
